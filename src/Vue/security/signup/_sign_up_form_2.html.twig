<form method="post" action="{{ getRoutePath('sign_up_check_pw') }}">
    <div class="uk-fieldset uk-margin-large-top">
        <!-- security code -->
        {%
            include 'form/_form_input.html.twig'
            with {'type': 'code', 'id': 'code', 'name': 'code', 'label': 'Security code', 'error': formCheck['code']}
        %}
    </div>
    <div class="uk-fieldset uk-margin-large-top">
        <!-- password -->
        {%
            include 'form/_form_input.html.twig'
            with {'type': 'password', 'id': 'password', 'name': 'password', 'label': 'Password', 'error': formCheck['password'], 'data': formData['password']}
        %}
        <!-- password confirm-->
        {%
            include 'form/_form_input.html.twig'
            with {'type': 'passwordConfirm', 'id': 'passwordConfirm', 'name': 'passwordConfirm', 'label': 'Confirm password', 'error': formCheck['passwordConfirm'], 'data': formData['passwordConfirm']}
        %}
    </div>
    <div class="uk-fieldset uk-margin-large-top">
        <!-- token -->
        <input type="hidden" name="token" value="{{ token }}">
        <!-- submit -->
        <div class="uk-margin">
            <button type="submit" class="uk-button uk-button-secondary uk-button-large uk-border-rounded uk-width-1-1" >
                Sign-up
            </button>
        </div>
    </div>
</form>